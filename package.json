{
    "name": "gledah",
    "version": "1.0.3",
    "description": "Sistema Facturacion y Administracion ",
    "main": "public/electron.js",
    "homepage": "./",
    "scripts": {
        "postinstall": "install-app-deps",
        "test": "echo \"Error: no test specified\" && exit 1",
        "pack": "electron-builder --dir",
        "dist": "electron-builder",
        "electron": "electron . ",
        "package-win": "electron-builder --win",
        "start": "react-scripts start",
        "build": "react-scripts build",
        "eject": "react-scripts eject",
        "lint": "eslint --ext .js,.jsx ./src",
        "lint:fix": "eslint --fix --ext .js,.jsx ./src",
        "clear-all": "rm -rf build node_modules",
        "re-start": "rm -rf build node_modules && yarn install && yarn start",
        "re-build": "rm -rf build node_modules && yarn install && yarn build",
        "preelectron-pack": "yarn build",
        "wdio": "wdio run wdio.conf.js"
    },
    "build": {
        "appId": "com.gledah",
        "directories": {
            "buildResources": "assets"
        },
        "win": {
            "target": "NSIS",
            "icon": "imageInst/logoints.png"
        },
        "publish": [
            {
                "provider": "github",
                "owner": "zerontec",
                "repo": "deskGledah"
            }
        ],
        "files": [
            "backend/**/*",
            "build/**/*",
            "node_modules/**/*",
            "package.json",
            "src/assets",
            "public/assets"
        ],
        "asar": true
    },
    "author": "Zeron Tecnologia <devsignweb@gmail.com> ",
    "license": "ISC",
    "assets": [
        "assets"
    ],
    "eslintConfig": {
        "extends": [
            "react-app"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-react"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@faker-js/faker": "^7.6.0",
        "@iconify/react": "^4.1.0",
        "@mui/icons-material": "^5.11.16",
        "@mui/lab": "^5.0.0-alpha.103",
        "@mui/material": "^5.11.10",
        "@railway/cli": "^3.3.1",
        "@react-pdf/render": "^3.2.6",
        "@react-pdf/renderer": "^3.1.12",
        "antd": "^5.6.1",
        "apexcharts": "^3.37.0",
        "axios": "^1.4.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "caniuse-lite": "^1.0.30001499",
        "change-case": "^4.1.2",
        "consulta-dolar-venezuela": "^1.0.4",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "dotenv": "^16.3.1",
        "electron-is-dev": "^2.0.0",
        "electron-log": "^4.4.8",
        "electron-updater": "^6.1.1",
        "express": "^4.16.4",
        "express-validator": "^6.0.0",
        "history": "^5.3.0",
        "iconv-lite": "^0.6.3",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.28",
        "lodash": "^4.17.21",
        "mongoose": "^5.9.12",
        "morgan": "^1.10.0",
        "nodemon": "^2.0.4",
        "numeral": "^2.0.6",
        "passport": "^0.4.0",
        "passport-jwt": "^1.0.0",
        "pdf-lib": "^1.17.1",
        "pg": "^8.7.3",
        "prop-types": "^15.8.1",
        "react": "^18.2.0",
        "react-apexcharts": "^1.4.0",
        "react-day-picker": "^8.7.1",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.3.0",
        "react-hook-form": "^7.43.1",
        "react-redux": "^8.0.7",
        "react-router-dom": "^6.8.1",
        "react-scripts": "^5.0.1",
        "react-validation": "^3.0.7",
        "redux": "^4.2.1",
        "redux-devtools-extension": "^2.13.9",
        "redux-thunk": "^2.4.2",
        "request-promise": "^4.2.0",
        "sequelize": "^6.21.3",
        "simplebar-react": "^3.2.1",
        "socket.io": "^4.6.1",
        "socket.io-client": "^4.7.1",
        "styled-components": "^5.3.11",
        "sweetalert2": "^11.7.10",
        "validator": "^13.9.0",
        "web-vitals": "^3.1.1",
        "winston": "^3.2.1",
        "ws": "^8.13.0",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@babel/core": "^7.21.0",
        "@babel/eslint-parser": "^7.19.1",
        "@wdio/cli": "^8.13.12",
        "@wdio/local-runner": "^8.13.12",
        "@wdio/mocha-framework": "^8.12.1",
        "@wdio/spec-reporter": "^8.12.2",
        "chromedriver": "^115.0.0",
        "electron": "^25.3.1",
        "electron-builder": "^23.6.0",
        "electron-is-dev": "^2.0.0",
        "eslint": "^8.34.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-prettier": "^8.6.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-flowtype": "^8.0.3",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^2.8.4",
        "wdio-chromedriver-service": "^8.1.1"
    },
    "overrides": {
        "nth-check": "2.1.1"
    }
}
